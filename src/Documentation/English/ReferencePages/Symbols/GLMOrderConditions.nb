Cell[TextData[{
 "The stage order ",
 Cell[BoxData[
  FormBox["k", TraditionalForm]], "InlineMath",ExpressionUUID->
  "ce2bbd52-fcd9-430d-8a0b-e95c73629820"],
 " residual is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox[
     SuperscriptBox["c", "k"], 
     RowBox[{"k", "!"}]], "-", 
    RowBox[{"A", " ", 
     FractionBox[
      SuperscriptBox["c", 
       RowBox[{"k", "-", "1"}]], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"k", "-", "1"}], ")"}], "!"}]]}], "-", 
    RowBox[{"U", " ", 
     SubscriptBox["q", "k"]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"3acadb7a-8a81-4165-a740-9bcab2edf45d"],
 " for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"k", ">", "0"}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "16f028c5-c6db-4ebb-af6c-d60fe6cafcb0"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{"e", "-", 
    RowBox[{"U", " ", 
     SubscriptBox["q", "0"]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"dd094311-05a1-488a-8941-f6af1de35d36"],
 " for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"k", "=", "0"}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "fec3c231-882d-4853-a235-552dcb071f14"],
 ", where ",
 Cell[BoxData[
  FormBox["e", TraditionalForm]], "InlineMath",ExpressionUUID->
  "c108ac9f-1b7d-4c44-b8a5-01ac119dbc18"],
 " is a vector of ",
 Cell[BoxData[
  FormBox["1", TraditionalForm]], "InlineMath",ExpressionUUID->
  "37849022-5f14-4c49-9994-3333504a9e6c"],
 "'s. The order ",
 Cell[BoxData[
  FormBox["k", TraditionalForm]], "InlineMath",ExpressionUUID->
  "0eb2da42-dcba-4ae4-acd5-2ac3bef7e9ac"],
 " residual is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     UnderoverscriptBox["\[Sum]", 
      RowBox[{"l", "=", "0"}], "k"], 
     FractionBox[
      SubscriptBox["q", 
       RowBox[{"k", "-", "l"}]], 
      RowBox[{"l", "!"}]]}], "-", 
    RowBox[{"B", 
     FractionBox[
      SuperscriptBox["c", 
       RowBox[{"k", "-", "1"}]], 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"k", "-", "1"}], ")"}], "!"}]]}], "-", 
    RowBox[{"V", " ", 
     SubscriptBox["q", "k"]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"98efafca-8cef-420b-a80c-7a6d458280cd"],
 " for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"k", ">", "0"}], TraditionalForm]], "InlineMath",ExpressionUUID->
  "439bf212-6e98-4aa6-9692-e2a3a7c2efba"],
 " and ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SubscriptBox["q", "0"], "-", 
    RowBox[{"V", " ", 
     SubscriptBox["q", "0"]}]}], TraditionalForm]], "InlineMath",
  ExpressionUUID->"575483be-1a23-4865-9869-2e19f60b2960"],
 " for ",
 Cell[BoxData[
