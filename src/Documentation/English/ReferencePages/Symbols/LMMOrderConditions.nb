Cell[TextData[{
 "The order condition residual of order ",
 Cell[BoxData[
  StyleBox["p", "TI"]], "InlineFormula",ExpressionUUID->
  "895a6f7a-1fa8-495f-9130-35d36d633b91"],
 " is ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    FractionBox["1", 
     RowBox[{"\[Sigma]", "(", "1", ")"}]], 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"i", "=", "0"}], "k"], 
       RowBox[{
        SubscriptBox["\[Alpha]", "i"], 
        FractionBox[
         SuperscriptBox["i", "p"], 
         RowBox[{"p", "!"}]]}]}], "-", 
      RowBox[{
       SubscriptBox["\[Beta]", "i"], 
       FractionBox[
        RowBox[{"p", " ", 
         SuperscriptBox["i", 
          RowBox[{"p", "-", "1"}]]}], 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"p", "-", "1"}], ")"}], "!"}]]}]}], ")"}]}], 
   TraditionalForm]], "InlineMath",ExpressionUUID->
  "ad486bdc-9aac-429e-854e-2462188dff40"],
