Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["Y", "i"], "=", 
    RowBox[{
     SubscriptBox["y", "n"], "+", 
     RowBox[{"h", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"j", "=", "1"}], "s"], 
       RowBox[{
        SubscriptBox["a", 
         RowBox[{"i", ",", "j"}]], 
        RowBox[{
         StyleBox["f", "TI"], 
         StyleBox["(", "TI"], 
         RowBox[{
          RowBox[{
           SubscriptBox[
            StyleBox["t", "TI"], "n"], "+", 
           RowBox[{
            SubscriptBox["c", "j"], "h"}]}], ",", 
          SubscriptBox["Y", "j"]}], 
         StyleBox[")", "TI"]}]}]}]}]}]}], 
   StyleBox[",", "TI"], 
   StyleBox["\[IndentingNewLine]", "TI"], 
   RowBox[{
    SubscriptBox[
     StyleBox["y", "TI"], 
     RowBox[{"n", "+", "1"}]], "=", 
    RowBox[{
     SubscriptBox["y", "n"], "+", 
     RowBox[{"h", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"j", "=", "1"}], "s"], 
       RowBox[{
        SubscriptBox["b", "j"], 
        RowBox[{
         RowBox[{
          StyleBox["f", "TI"], 
          StyleBox["(", "TI"], 
          RowBox[{
           RowBox[{
            SubscriptBox[
             StyleBox["t", "TI"], "n"], "+", 
            RowBox[{
             SubscriptBox["c", "j"], "h"}]}], ",", 
           SubscriptBox["Y", "j"]}], 
          StyleBox[")", "TI"]}], 
         StyleBox[".", "TI"]}]}]}]}]}]}]}], 
  TraditionalForm]], \
"InlineMath",ExpressionUUID->"fba73540-ee48-482a-bc13-2c236699d3e9"]], "Text",\

 CellChangeTimes->{{3.879689081574112*^9, 3.879689190568797*^9}, {
  3.879689224091873*^9, 3.879689267311903*^9}},
 CellID->1950070479,ExpressionUUID->"5f40eed4-f4f6-4fa2-8e1d-a7807aadb1f4"],

Cell[TextData[{
 "where ",
 Cell[BoxData[
  FormBox["h", TraditionalForm]], "InlineMath",ExpressionUUID->
  "14be3985-da6c-4ca0-82bc-3987c2e7c678"],
